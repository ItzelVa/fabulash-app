    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="288dp"
            android:height="288dp"
            android:viewportWidth="288"
            android:viewportHeight="288">
            <group
                android:name="group"
                android:pivotX="144"
                android:pivotY="144"
                android:scaleX="0.85"
                android:scaleY="0.85">
                <path
                    android:name="path"
                    android:pathData="M 77.441 98 C 77.855 101.941 81.044 111.792 81.576 112.777 C 82.107 113.762 84.883 122.136 97.285 134.451 C 109.989 147.064 121.676 149.074 131.184 151.198 C 137.799 152.676 147.39 152.971 154.335 152.183 C 161.281 151.395 167.151 150.213 179.553 143.317 C 187.276 139.023 194.849 131.495 197.33 128.54 C 199.201 126.309 209.732 112.777 210.145 98"
                    android:strokeColor="#f5efe4"
                    android:strokeWidth="9"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"/>
                <path
                    android:name="path_3"
                    android:pathData="M 70 161.05 L 84.166 144.444 M 119.609 189.619 L 124.68 167.946 M 168.391 189.619 L 164.257 168.931 M 218 160.065 L 203.944 144.302"
                    android:fillColor="#000"
                    android:strokeColor="#f5efe4"
                    android:strokeWidth="9"
                    android:strokeLineCap="round"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="200"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="200"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="250"
                    android:duration="350"
                    android:valueFrom="0"
                    android:valueTo="360"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
